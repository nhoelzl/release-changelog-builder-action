{
    "categories": [
        {
            "title": "## 🚀 Features",
            "labels": ["🚀", "🌟"]
        },
        {
            "title": "## 🐛 Fixes",
            "labels": ["🐛"]
        },
        {
            "title": "## 🧪 Tests",
            "labels": ["🧪"]
        },
        {
            "title": "## 💬 Other",
            "labels": ["💬", "📖", "🚨"]
        },
        {
            "title": "## 📦 Dependencies",
            "labels": ["dependencies"]
        }
    ],
    "pr_template": "${{TITLE}}",
    "label_extractor": [
        {
            "pattern": "(.) (.+)",
            "target": "$1"
        }
    ],
    "transformers": [
        {
            "pattern": "(.) (.+)",
            "target": "- $2"
        }
    ]
}